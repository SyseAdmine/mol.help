<h1 id="hd_id3152598"><variable id="wie">To access this function...</variable></h1>
  <section id="next_marker">
<paragraph role="paragraph" id="par_id3153415">Choose <menuitem>Edit - Next Marker</menuitem>.</paragraph>
<paragraph role="paragraph" id="par_id3146971"><keycode>F4</keycode> key</paragraph>.
  </section>
  <section id="previous_marker">
<paragraph role="paragraph" id="par_id3149021">Choose <menuitem>Edit - Previous Marker</menuitem>.</paragraph>
<paragraph role="paragraph" id="par_id3153966"><ekycode>Shift+F4</keycode>.</paragraph>
  </section>
  <section id="next_error">
<paragraph role="paragraph" id="par_id3154020">Choose <menuitem>Edit - Next Error</menuitem>.</paragraph>
<paragraph role="paragraph" id="par_id3150205"><keycode>F3</keycode> key</paragraph>.
  </section>
  <section id="previous_error">
<paragraph role="paragraph" id="par_id3149499">Choose <menuitem>Edit - Previous Error</menuitem>.</paragraph>
<paragraph role="paragraph" id="par_id3148870"><keycode>Shift+F3</keycode>.</paragraph>
  </section>
<paragraph role="paragraph" id="par_id3149127">On the <emph>Tools</emph> bar, click</paragraph>
  <section id="syzoom100">
    <table id="tbl_id3149599">
      <tablerow>
        <tablecell>
          <paragraph role="paragraph" id="par_id3154653"><image id="img_id3154765" src="cmd/lc_zoom100percent.svg" width="1cm" height="1cm"><alt id="alt_id3154765" xml-lang="en-US">Icon Zoom 100%</alt></image></paragraph>
        </tablecell>
        <tablecell>
          <paragraph role="paragraph" id="par_id3147298">Zoom 100%.</paragraph>
        </tablecell>
      </tablerow>
    </table>
  </section>
  <section id="zoom_in">
<paragraph role="paragraph" id="par_id3147407">Choose <menuitem>View - Zoom In</menuitem>.</paragraph>
<paragraph role="paragraph" id="par_id3156398">On the <emph>Tools</emph> bar, click</paragraph>
