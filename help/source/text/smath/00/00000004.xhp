      <table id="tbl_id3157875">
        <tablerow>
          <tablecell>
            <paragraph role="paragraph" id="par_id3150743"><image id="img_id3163822" src="cmd/lc_zoomin.svg" width="1cm" height="1cm"><alt id="alt_id3163822" xml-lang="en-US">Icon Zoom In</alt></image></paragraph>
