    <tablerow>
      <tablecell>
        <paragraph role="paragraph" id="par_idN10084" xml-lang="en-US"><image id="img_id3149801" src="media/helpimg/starmath/al21801.svg" width="1cm" height="1cm"><alt id="alt_id3149801" xml-lang="en-US">Icon Round brackets (parantheses)</alt></image></paragraph>
